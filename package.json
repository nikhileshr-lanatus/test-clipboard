{
    "name": "desktop-clipboard-app",
    "version": "1.0.0",
    "description": "Desktop-only online clipboard sharing with 4-digit codes",
    "main": "server/index.js",
    "scripts": {
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server": "nodemon server/index.js",
        "client": "cd client && npm start",
        "build": "cd client && npm run build",
        "start": "node server/index.js"
    },
    "dependencies": {
        "express": "^4.18.2",
        "ws": "^8.14.2",
        "multer": "^1.4.5-lts.1",
        "cors": "^2.8.5",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "concurrently": "^8.2.2"
    },
    "keywords": [
        "clipboard",
        "sharing",
        "websocket",
        "real-time"
    ],
    "author": "",
    "license": "MIT"
}